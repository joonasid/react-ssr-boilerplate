{
  "name": "react-ssr-boilerplate",
  "version": "1.0.0",
  "engines": {
    "yarn": "1.2.1",
    "node": ">=7.0.0"
  },
  "dependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react-app": "3.0.1",
    "babel-runtime": "6.23.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "memory-cache": "^0.2.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "node-object-hash": "^1.3.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "react": "15.6.1",
    "react-dev-utils": "3.0.2",
    "react-dom": "15.6.1",
    "react-error-overlay": "1.0.9",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-hotjar": "^0.2.0",
    "redux-little-router": "^14.2.0",
    "redux-saga": "^0.15.4",
    "reload": "^2.0.1",
    "style-loader": "0.18.2",
    "styled-components": "^2.1.1",
    "styled-reset": "0.1.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "tracer": "^0.8.9",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-shell-plugin": "^0.5.0",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "scripts": {
    "watch-client": "NODE_ENV=development webpack -w --config ./config/webpack.config.dev.js",
    "watch-server": "NODE_ENV=development BABEL_ENV=production TMPDIR=/tmp ./node_modules/.bin/babel-watch -w src/server/views -w .reload --exclude src/client src/server/index.js",
    "watch": "node_modules/.bin/concurrently --kill-others \"yarn watch-client\" \"(sleep 7 && yarn watch-server)\"",
    "build-client": "node scripts/build.js",
    "build-server": "BABEL_ENV=production ./node_modules/.bin/babel src -d dist --copy-files",
    "build": "yarn run clean && yarn build-client && yarn build-server",
    "clean": "rimraf dist build",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint src",
    "test": "jest"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "20.0.3",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.5.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "1.0.5",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "~20.0.4",
    "jsdom": "11.1.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "15.0.0-rc.2",
    "react-dom": "15.6.1",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "0.10.5",
    "rimraf": "^2.6.1",
    "simple-ngram-markov": "^1.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/__tests__/**/*.js?(x)",
      "<rootDir>/test/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
